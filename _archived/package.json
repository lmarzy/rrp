{
  "name": "kickstart-gulp",
  "version": "0.0.1",
  "description": "Starter template using Gulp as the build tool",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmarzy/kickstart-gulp.git"
  },
  "homepage": "https://github.com/lmarzy/kickstart-gulp#readme",
  "author": "Lee Marsden",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lmarzy/kickstart-gulp/issues"
  },
  "scripts": {
    "test": "mocha",
    "clean": "rm -rf dev/ dist/",
    "sass": "node-sass -o dev/css src/styles",
    "vr": "postcss -u postcss-vr -r dev/css/*.css",
    "autoprefixer": "postcss -u autoprefixer --autoprefixer.browsers 'last 2 version' -r dev/css/*.css",
    "mqpacker": "postcss -u css-mqpacker -r dev/css/*.css",
    "cssnano": "postcss -u cssnano -r dev/css/*.css",
    "devStyles": "npm run sass && npm run vr && npm run autoprefixer",
    "prodStyles": "npm run mqpacker && npm run cssnano",
    "dev": "npm run clean && gulp serve",
    "prod": "npm run clean && gulp --prod && rm -rf dev/"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^12.0.2",
    "chai": "^3.5.0",
    "css-mqpacker": "^4.0.0",
    "cssnano": "^3.5.2",
    "glob": "^5.0.15",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-data": "^1.2.1",
    "gulp-eslint": "^1.1.1",
    "gulp-front-matter": "^1.3.0",
    "gulp-html-validator": "0.0.5",
    "gulp-htmlmin": "^1.3.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-nunjucks-html": "^2.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.0",
    "gulp-rev-all": "^0.8.24",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^2.2.0",
    "gulp-sitemap": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-streamify": "^1.0.2",
    "gulp-stylus": "^2.3.1",
    "gulp-uglify": "^1.5.3",
    "gulp-uncss": "^1.0.4",
    "gulp-util": "^3.0.7",
    "mocha": "^2.4.5",
    "node-sass": "^4.7.2",
    "postcss-cli": "^2.5.1",
    "postcss-focus": "^1.0.0",
    "postcss-reporter": "^1.3.3",
    "postcss-scss": "^0.1.7",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "stylelint": "^5.1.0",
    "stylelint-config-standard": "^4.0.1",
    "vinyl-source-stream": "^1.1.0"
  }
}
